        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sequence ALingment Ontology for pairwise and multiple sequence alignments.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SALON</rdfs:label>
    </owl:Class>
    


